
    <div class="signup su-step1 js-step1">

        <div class="su-header">

            <div class="su-mask"></div>

            <div class="su-header-content">
                <form class="su-profile-pic sp-signup sp-signup-man-white js-image-upload" method="post" action="upload/profile/image" enctype="multipart/form-data" target="image-upload-helper">
                    {{?it.profile_image}}
                      <div class="image-thumbnail-preview final">
                        <img class="image-thumbnail-prefilled" src="{{=it.profile_image}}"></img>
                      </div>
                    {{??}}
                      <div class="image-thumbnail-preview initial">
                      </div>
                    {{?}}
                    <input name="image" class="su-upload js-step1-picture js-signup-help" data-help="pictures" type="file" />

                </form>
                <iframe class="hidden" name="image-upload-helper" id="image-upload-helper"></iframe>
                <h2 class="su-cover-name js-cover-name"></h2>
                {{?it.user_name}}
                  <h3 class="su-cover-url">{{=it.user_name}} (<span class="su-link js-step1-quit">not you?</span>)</h3>
                {{?}}
                <span class="su-cover-pic">
                  <i class="su-camera sp-signup sp-signup-camera"></i> Add a cover picture
                  <input class="su-upload js-step1-cover js-signup-help" data-help="pictures" type="file" />
                </span>
            </div>

        </div>

        <form class="su-form su-prefilled js-signup-form">

            <div class="su-form-content">
                <label class="form-label su-label" for="fullname">
                    Full name
                    <input tabindex="1" class="su-input form-input js-signup-fullname" data-help="name" type="text" name="fullname" />
                </label>

                <label class="form-label su-label-gender" for="gender">
                    Gender
                    <select tabindex="1" class="su-input form-input js-signup-gender" name="gender">
                        {{?it.gender === 'male'}}
                          <option value="-">-</option>
                          <option value="female">Female</option>
                          <option value="male" selected>Male</option>
                        {{??it.gender === 'female'}}
                          <option value="-">-</option>
                          <option value="female" selected>Female</option>
                          <option value="male">Male</option>
                        {{??}}
                          <option value="-">-</option>
                          <option value="female">Female</option>
                          <option value="male">Male</option>
                        {{?}}
                    </select>
                </label>



                <div class="form-label su-label-birthdate">
                    <span class="su-birthdate">Birthdate</span>
                    <input tabindex="1" name="date" class="su-input-date form-input input-date-mobile js-signup-date" data-help="birthdate" type="date" placeholder="MM-DD-YYY" />
                    <input name="month" tabindex="1" class="su-input form-input input-date-desktop js-signup-month" data-help="birthdate" type="text" placeholder="MM" />
                    <input name="day" tabindex="1" class="su-input form-input input-date-desktop js-signup-day" data-help="birthdate" type="text" placeholder="DD" />
                    <input name="year" tabindex="1" class="su-input-year form-input input-date-desktop js-signup-year" data-help="birthdate" type="text" placeholder="YYYY" />
                </div>


                <label class="form-label su-label" for="email">
                    Confirm your email<sup>*</sup>
                    <input tabindex="1" class="su-input form-input js-signup-email" data-help="email" type="email" name="email"/>
                </label>

                <label class="form-label su-label" for="password">
                    Set a password<sup>*</sup>
                    <input type="password" tabindex="1" class="su-input form-input js-signup-password" data-help="password" type="password" name="password" />
                </label>

                <div class="su-error-message js-signup-error-message hidden">
                  <i class="sp-signup-warning sp-signup su-error-message-warning-icon"></i>
                  <p class="su-error-message-text js-signup-error-message-text"></p>
                </div>

            </div>

            <div class="su-form-footer">

                <div class="su-terms">

                    By clicking "Join now" you agree to P1's <span class="su-link js-step1-terms">terms of usage</span> and <span class="su-link js-step1-privacy">privacy policy</span>.

                </div>

                <input type="submit" tabindex="1" class="button-blue su-submit js-step1-submit" data-help="submit" value="Join now"></input>

            </div>
            {{?it.profile_image}}
              <input name="profile-image"type="hidden" class="js-signup-thumbnail" value="{{!it.profile_image_data}}"></input>
            {{??}}
              <input name="profile-image"type="hidden" class="js-signup-thumbnail"></input>
            {{?}}
            {{?it.weibo_uid}}
              <input name="weibo-uid" type="hidden" value="{{!it.weibo_uid}}"></input>
            {{?}}
        </form>

    </div>
